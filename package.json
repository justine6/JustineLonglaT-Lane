{
  "name": "jutellane-main",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "validate:links": "node tools/validate-links.mjs",
    "lint": "eslint .",
    "prepare": "husky",
    "check-all": "node tools/run-ps.cjs scripts/check-dictionaries.ps1 && node tools/run-ps.cjs scripts/validate-and-repair-json.ps1",
    "patch-missing": "node tools/run-ps.js scripts/patch-missing.ps1",
    "fix-footer": "node tools/run-ps.js scripts/fix-footer.ps1",
    "patch-all": "npm run patch-missing && npm run fix-footer",
    "repair": "npm run patch-all && npm run check-all",
    "repair:quick": "npm run patch-all && npm run check-all",
    "sync": "node tools/run-ps.js scripts/sync-dicts.ps1",
    "sync:verbose": "node tools/run-ps.js scripts/sync-dicts.ps1 -ShowSkipped",
    "update-dicts:full": "node tools/run-ps.js scripts/Update-Dictionaries-Full.ps1",
    "update-dicts:incremental": "node tools/run-ps.js scripts/Update-Dictionaries-Incremental.ps1",
    "generate-types": "node tools/run-ps.js scripts/generate-types.ps1",
    "test:run": "vitest run",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "coverage:strict": "vitest run --coverage --passWithNoTests=false",
    "ci-check": "npm run repair && npm run lint && npm run coverage:strict",
    "ci-repair": "npm run repair && npm run check-all && npm run lint && npm run coverage:strict",
    "test:ci": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/eslint-plugin-next": "^15.5.6",
    "@next/mdx": "^15.5.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.3",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "@vercel/blob": "^2.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.545.0",
    "marked": "^17.0.1",
    "next": "15.5.7",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "react": "^19.2.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-scrollspy": "^3.4.3",
    "resend": "^6.4.2",
    "swiper": "^12.0.2",
    "zod": "^4.1.12"
  }
}

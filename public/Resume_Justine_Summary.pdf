  <div style="display: grid; gap: 12px; align-items: center; grid-template-columns: 1fr auto;">
    <small style="color:#64748b">PDF preview with download</small>
    <div style="display:flex; gap:8px;">
      <a href="/Resume_Justine.pdf" download class="btn">Download PDF</a>
      <button onclick="document.getElementById('resume-full').contentWindow.print()">Print</button>
    </div>
  </div>

  <div style="border:1px solid #e2e8f0; border-radius:12px; overflow: hidden; margin-top:10px;">
